{
	"dependencies": {
		"@babel/runtime": "~7.28.4",
		"@expo/metro-runtime": "~6.1.2",
		"@tailwindcss/postcss": "~4.1.13",
		"expo": "~54.0.10",
		"expo-apple-authentication": "~8.0.7",
		"expo-auth-session": "~7.0.8",
		"expo-blur": "~15.0.7",
		"expo-router": "~6.0.8",
		"expo-secure-store": "~15.0.7",
		"expo-splash-screen": "~31.0.10",
		"expo-status-bar": "~3.0.8",
		"expo-system-ui": "~6.0.7",
		"expo-updates": "~29.0.11",
		"radon-ide": "~0.0.1",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.4",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-reanimated": "~4.1.0",
		"react-native-safe-area-context": "5.4.0",
		"react-native-screens": "~4.16.0",
		"react-native-web": "~0.21.1",
		"react-native-worklets": "0.5.1",
		"tailwindcss": "~4.1.13",
		"uniwind": "^1.0.0-beta.4"
	},
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0"
  },
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"listUnknownPackages": false
			}
		}
	},
	"main": "expo-router/entry",
	"name": "uniwind-app",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome"
		]
	},
	"private": true,
	"scripts": {
		"android": "expo run:android",
		"convex:dev": "npx convex dev",
		"detox:build:ios:debug": "detox build -c ios.sim.debug ",
		"detox:build:ios:release": "detox build -c ios.sim.release",
		"detox:test:android:debug": "bun x detox test -c android.sim.debug --record-videos all",
		"detox:test:android:release": "bun x detox test -c android.sim.release --record-videos all",
		"detox:test:ios:debug": "bun x detox test -c ios.sim.debug --record-videos all",
		"detox:test:ios:release": "bun x detox test -c ios.sim.release --record-videos all",
		"dev": "expo start --dev-client",
		"fix": "biome check --write",
		"ios": "expo prebuild && expo run:ios",
		"typecheck": "tsgo",
		"web": "expo start --web"
	},
	"version": "1.0.0"
}
